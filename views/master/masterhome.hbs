 <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.4.1/jquery.min.js"></script>

<script>
  $ (document) .ready (function () {
       $(".aggiungiUserRicerca").hide();
       $(".cards").hide();
$(".aggiungiUser").click(function(e){ 
  $(".aggiungiUserRicerca").show();
    $(".partyAttivi").hide();
    $(".cards").hide();
   e.preventDefault();
},
);
    });
$ (document) .ready (function () {
$(".collezioneParty").click(function(e){ 
  $(".aggiungiUserRicerca").hide();
  $(".cards").hide();
    $(".partyAttivi").show();
   e.preventDefault();
},
);
    });

    $ (document) .ready (function () {
$(".schedePers").click(function(e){ 
  $(".aggiungiUserRicerca").hide();
      $(".cards").show();
    $(".partyAttivi").hide();
   e.preventDefault();
},
);
    });


</script>