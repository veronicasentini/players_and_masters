<div class="contenuti">
<form  method="POST"  action="/user/master/aggiungiParty"class="partyAttivi" > 

  <div class="creaParty" >
  <p>Nome party: <input class="nomeParty" name="nomePartyMaster"> 
  <!--<a href="/user/master/aggiungiParty" style="background-color: darkorange; border-color:black; border-weight:2px;">aggiungi Party</a>-->
  </p> <button type="submit" class="bottone-agg-party">aggiungi party</button></div>
 
  <div id="griglia-personaggi" class="clearfix">
 <div id="intestazioneForm">
    <p >party attivi</p>
</div>
<table style="background-color:rgba(240, 248, 255, 0.5); width:100%">
  <tbody>
    <tr style="background-color: darkorange">
      <td>nome party</td>
    </tr>
{{#each partyAttivi}}
<tr class="partyAttivo"><td>{{this.name}}</td></tr>
{{/each}}
  </tbody>
</table>
</div>
</form>
<form class="aggiungiUserRicerca " >
  <div id="ricerca" class="clearfix">
    <div id="intestazioneForm" style="font-size:25px" >Ricerca players</div>
   <div class="filtri all_orizz" style="background-color:rgba(240, 248, 255, 0.5); ">
      <div style="margin-left: 5px;">
       <p style="font-weight: bold; font-size:20px">FILTRI</p>

  <!-- CASELLE DI TESTO -->

 <p style="font-style: italic; font-size:18px; ">Nome Utente<br>
  <input type="text" class="mail-password" name="username"></p>
  
  <!-- CHECKBOX -->
  <!--<p style="font-style: italic; font-size:18px; ">Classe</p>-->

</div> 
  <div id="cerca"> <button type="submit" name="cerca" class="bottone-conferma" >Cerca</button></div>
 
</div>
<div id="risultato"  class="all_orizz risultato">
